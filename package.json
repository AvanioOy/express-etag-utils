{
    "name": "@avanio/express-etag-utils",
    "version": "0.0.1",
    "description": "ExpressJS ETag utils",
    "main": "dist/index.js",
    "directories": {
        "test": "test"
    },
    "mocha": {
        "exit": true,
        "extension": [
            "ts",
            "js"
        ],
        "recursive": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ],
        "reporters": [
            "spec",
            "mocha-junit-reporter"
        ]
    },
    "nyc": {
        "extension": [
            ".ts"
        ],
        "include": [
            "src"
        ],
        "reporter": [
            "text",
            "html"
        ],
        "all": true
    },
    "scripts": {
        "test": "cross-env TS_NODE_PROJECT='./tsconfig.test.json' nyc mocha"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AvanioOy/express-etag-utils.git"
    },
    "keywords": [
        "etag",
        "express"
    ],
    "author": "mharj",
    "license": "LGPL-2.1-only",
    "bugs": {
        "url": "https://github.com/AvanioOy/express-etag-utils/issues"
    },
    "homepage": "https://github.com/AvanioOy/express-etag-utils#readme",
    "peerDependencies": {
        "express": ">=4.0.0"
    },
    "devDependencies": {
        "@types/chai": "^4.3.3",
        "@types/etag": "^1.8.1",
        "@types/express": "^4.17.13",
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.7.14",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "chai": "^4.3.6",
        "cross-env": "^7.0.3",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-sonarjs": "^0.15.0",
        "express": "^4.18.1",
        "mocha": "^10.0.0",
        "nyc": "^15.1.0",
        "prettier": "^2.7.1",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.2"
    },
    "dependencies": {
        "etag": "^1.8.1"
    }
}
